{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201 --ssl true --ssl-cert ssl/server.crt --ssl-key ssl/server.key -o",
    "build": "ng build",
    "build:test": "ng build --configuration test",
    "build:prod": "ng build --configuration production",
    "build:githubpages": "ng build --configuration test --output-path docs --base-href /CastlesAndNest-Frontend/",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/Frontend/stats.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "how to deploy to firebase": "run for prod using the above script. In the terminal 'firebase deploy'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/cdk": "^12.2.10",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/localize": "~12.1.1",
    "@angular/material": "^12.2.10",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@auth0/angular-jwt": "^5.0.2",
    "@ckeditor/ckeditor5-angular": "^2.0.2",
    "@ckeditor/ckeditor5-build-classic": "^30.0.0",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "angular-cc-library": "^2.1.2",
    "angularx-social-login": "^4.0.1",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.5.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.3.2",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
